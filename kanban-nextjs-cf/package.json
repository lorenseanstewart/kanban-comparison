{
  "name": "kanban-nextjs",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "npm run build && wrangler pages deploy .vercel/output/static",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 execute kanban-db --local --file=drizzle/migrations/<migration-file>.sql",
    "db:migrate:remote": "wrangler d1 execute kanban-db --remote --file=drizzle/migrations/<migration-file>.sql",
    "db:seed": "node --import tsx/esm src/db/seed.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "browserslist-config-baseline": "^0.5.0",
    "charts.css": "^1.2.0",
    "daisyui": "^5.1.25",
    "drizzle-orm": "^0.44.7",
    "next": "16.0.0-beta.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250102.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^20",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^11.8.1",
    "cssnano": "^7.1.2",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9",
    "eslint-config-next": "16.0.0-beta.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "wrangler": "^4.0.0"
  },
  "overrides": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
