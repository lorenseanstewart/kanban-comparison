{
  "name": "my-qwik-empty-starter",
  "description": "Blank project with routing included",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "npm run pages:build && wrangler pages deploy dist",
    "dev": "npm run pages:build && wrangler pages dev dist --d1=DB=kanban-db --live-reload",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "pages:build": "rm -rf dist && npm run build.client && npm run build.server && rm -f dist/_worker.js && cp -r .cloudflare/* dist/ && cp -r functions dist/",
    "pages:deploy": "npm run pages:build && wrangler pages deploy dist",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 execute kanban-db --file=./drizzle/migrations/0000_narrow_kylun.sql",
    "db:migrate:local": "wrangler d1 execute kanban-db --local --file=./drizzle/migrations/0000_narrow_kylun.sql",
    "db:push": "drizzle-kit push",
    "db:reset": "node scripts/reset-db.js",
    "seed": "wrangler d1 execute kanban-db --file=./scripts/seed-d1.sql",
    "seed:local": "wrangler d1 execute kanban-db --local --file=./scripts/seed-d1.sql",
    "setup": "bash scripts/seed-dev.sh",
    "setup:remote": "npm run db:migrate && npm run seed"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.16.1",
    "@builder.io/qwik-city": "^1.16.1",
    "@cloudflare/workers-types": "^4.20251014.0",
    "@eslint/js": "latest",
    "@types/node": "20.19.0",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.31.5",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "^1.16.1",
    "globals": "16.3.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "latest",
    "undici": "*",
    "vite": "7.1.4",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^4.45.3"
  },
  "dependencies": {
    "charts.css": "^1.2.0",
    "daisyui": "^5.1.26",
    "drizzle-orm": "^0.44.5",
    "valibot": "^1.1.0"
  }
}
