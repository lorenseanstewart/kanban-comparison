{
  "name": "kanban-comparison",
  "version": "1.0.0",
  "description": "Framework comparison project: Same app built in 7 frameworks",
  "private": true,
  "scripts": {
    "measure:single": "tsx scripts/measure-single.ts",
    "measure:final": "tsx scripts/measure-final.ts --runs 5",
    "measure:all": "npm run measure:final && npm run generate:charts",
    "measure:legacy": "tsx scripts/measure-all-lighthouse.ts",
    "generate:charts": "python3 scripts/generate-charts.py",
    "build:all": "for dir in kanban-*/; do (cd \"$dir\" && npm run build); done"
  },
  "keywords": [
    "react",
    "vue",
    "svelte",
    "solid",
    "qwik",
    "angular",
    "framework-comparison",
    "performance"
  ],
  "author": "Loren Stewart",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prettier": "^3.6.2",
    "prettier-plugin-marko": "^3.3.2",
    "tsx": "^4.7.0"
  }
}
