{
  "name": "kanban-marko",
  "description": "The default Marko starter app with TypeScript",
  "version": "1.0.0",
  "type": "module",
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@formkit/drag-and-drop": "^0.5.3",
    "@types/better-sqlite3": "^7.6.13",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^12.4.1",
    "charts.css": "^1.2.0",
    "daisyui": "^5.1.31",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "marko": "^6.0.0",
    "tsx": "^4.20.6",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@marko/run": "^0.8.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  },
  "private": true,
  "scripts": {
    "dev": "marko-run",
    "build": "marko-run build",
    "preview": "marko-run preview",
    "start": "node --enable-source-maps ./dist/index.mjs",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:reset": "rm -f drizzle/db.sqlite drizzle/db.sqlite-shm drizzle/db.sqlite-wal",
    "seed": "tsx src/db/seed.ts",
    "setup": "npm run db:reset && npm run db:migrate && npm run seed"
  }
}
